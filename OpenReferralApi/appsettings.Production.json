{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Specification": {
    "BaseUrl": "https://raw.githubusercontent.com/tpximpact/OpenReferralApi/refs/heads/main/OpenReferralApi/Schemas/"
  },
  "Database": {
    "ConnectionString": "",
    "DatabaseName": "openreferral_prod",
    "ServicesCollection": "services"
  },
  "FeedValidation": {
    "Enabled": true,
    "IntervalHours": 48,
    "RunAtMidnight": true
  },
  "Security": {
    "AllowedCorsOrigins": [
      "https://openreferraluk.org",
      "https://www.openreferraluk.org",
      "https://api.openreferraluk.org"
    ],
    "ValidateSslCertificates": true
  },
  "RateLimiting": {
    "PermitLimit": 100,
    "Window": 60,
    "QueueLimit": 0
  },
  "OpenTelemetry": {
    "Enabled": true,
    "OtlpEndpoint": ""
  },
  "Cache": {
    "Enabled": true,
    "ExpirationMinutes": 120,
    "MaxSizeMB": 200,
    "UseSlidingExpiration": false,
    "SlidingExpirationMinutes": 30
  }
}